function trajectory = swimwOut(locx, locy, speedSwim)
% Use linspace to create a line the friends will follow to swim out of the
% Game

%     trajectory = [linspace(locx, -50, speedSwim); repmat(locy, 1, speedSwim)];
    trajectory = [linspace(locx, -50, speedSwim); repmat(locy, 1, speedSwim)];


end